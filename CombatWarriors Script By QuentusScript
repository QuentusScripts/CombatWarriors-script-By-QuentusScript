_G.HeadSize=15;_G.Disabled=true;game:GetService('RunService').RenderStepped:connect(function()if _G.Disabled then for a,a in next,game:GetService('Players'):GetPlayers()do if a.Name~=game:GetService('Players').LocalPlayer.Name then pcall(function()a.Character.Head.Size=Vector3.new(_G.HeadSize,_G.HeadSize,_G.HeadSize)a.Character.Head.Transparency=1;a.Character.Head.BrickColor=BrickColor.new("Red")a.Character.Head.Material="Neon"a.Character.Head.CanCollide=false;a.Character.Head.Massless=true end)end end end end)
